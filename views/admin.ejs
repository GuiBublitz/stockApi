<div class="container mt-5">
<h1>User List</h1>
<table class="table table-striped">
    <thead>
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Admin Status</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach(user => { %>
        <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.isAdmin ? 'Yes' : 'No' %></td>
        </tr>
    <% }) %>
    </tbody>
</table>
</div>